#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/qa_assessment_project/node_modules/.pnpm/@remix-run+dev@2.17.0_@remi_68fd83952107eb3dbd37e2fa9b35a929/node_modules/@remix-run/dev/dist/node_modules:/mnt/d/qa_assessment_project/node_modules/.pnpm/@remix-run+dev@2.17.0_@remi_68fd83952107eb3dbd37e2fa9b35a929/node_modules/@remix-run/dev/node_modules:/mnt/d/qa_assessment_project/node_modules/.pnpm/@remix-run+dev@2.17.0_@remi_68fd83952107eb3dbd37e2fa9b35a929/node_modules/@remix-run/node_modules:/mnt/d/qa_assessment_project/node_modules/.pnpm/@remix-run+dev@2.17.0_@remi_68fd83952107eb3dbd37e2fa9b35a929/node_modules:/mnt/d/qa_assessment_project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/qa_assessment_project/node_modules/.pnpm/@remix-run+dev@2.17.0_@remi_68fd83952107eb3dbd37e2fa9b35a929/node_modules/@remix-run/dev/dist/node_modules:/mnt/d/qa_assessment_project/node_modules/.pnpm/@remix-run+dev@2.17.0_@remi_68fd83952107eb3dbd37e2fa9b35a929/node_modules/@remix-run/dev/node_modules:/mnt/d/qa_assessment_project/node_modules/.pnpm/@remix-run+dev@2.17.0_@remi_68fd83952107eb3dbd37e2fa9b35a929/node_modules/@remix-run/node_modules:/mnt/d/qa_assessment_project/node_modules/.pnpm/@remix-run+dev@2.17.0_@remi_68fd83952107eb3dbd37e2fa9b35a929/node_modules:/mnt/d/qa_assessment_project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@remix-run/dev/dist/cli.js" "$@"
else
  exec node  "$basedir/../@remix-run/dev/dist/cli.js" "$@"
fi
