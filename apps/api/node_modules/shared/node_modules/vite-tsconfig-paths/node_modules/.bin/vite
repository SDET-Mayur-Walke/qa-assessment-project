#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/qa_assessment_project/node_modules/.pnpm/vite@7.1.5_@types+node@24.4_d7a015cf87d376bf4c5e60d86616bf83/node_modules/vite/bin/node_modules:/mnt/d/qa_assessment_project/node_modules/.pnpm/vite@7.1.5_@types+node@24.4_d7a015cf87d376bf4c5e60d86616bf83/node_modules/vite/node_modules:/mnt/d/qa_assessment_project/node_modules/.pnpm/vite@7.1.5_@types+node@24.4_d7a015cf87d376bf4c5e60d86616bf83/node_modules:/mnt/d/qa_assessment_project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/qa_assessment_project/node_modules/.pnpm/vite@7.1.5_@types+node@24.4_d7a015cf87d376bf4c5e60d86616bf83/node_modules/vite/bin/node_modules:/mnt/d/qa_assessment_project/node_modules/.pnpm/vite@7.1.5_@types+node@24.4_d7a015cf87d376bf4c5e60d86616bf83/node_modules/vite/node_modules:/mnt/d/qa_assessment_project/node_modules/.pnpm/vite@7.1.5_@types+node@24.4_d7a015cf87d376bf4c5e60d86616bf83/node_modules:/mnt/d/qa_assessment_project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../vite/bin/vite.js" "$@"
fi
